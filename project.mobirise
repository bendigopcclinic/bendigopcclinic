          "_customHTML": "<section class=\"extRestaurantMenu restaurant-special-menu\" group=\"Additional Blocks\" data-bg-video=\"{{bg.type == 'video'&&bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"mbr-restaurant-special-menu\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"1\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"0\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"ShowUnderline\" name=\"showUnderline\">\n        <input type=\"color\" title=\"Underlines Color\" name=\"underlineColor\" value=\"#ff3366\" condition=\"showUnderline\">\n\n        <input type=\"color\" title=\"Special Item\" name=\"specialColor\" value=\"#ff3366\">\n        <select title=\"Groups\" name=\"groups\">\n             <option value=\"2\" selected>2</option>\n             <option value=\"3\">3</option>\n             <option value=\"4\">4</option>\n             <option value=\"5\">5</option>\n             <option value=\"6\">6</option>\n        </select>\n        <input type=\"range\" title=\"Group1 Items\" inline name=\"group1Items\" min=\"1\" max=\"50\" value=\"4\" step=\"1\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup1\" value=\"0\">\n\n        <input type=\"range\" title=\"Group2 Items\" inline name=\"group2Items\" min=\"1\" max=\"50\" value=\"4\" step=\"1\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup2\" value=\"0\">\n\n        <input type=\"range\" title=\"Group3 Items\" inline name=\"group3Items\" min=\"1\" max=\"50\" value=\"3\" step=\"1\" condition=\"groups >2\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup3\" value condition=\"groups>2\">\n\n        <input type=\"range\" title=\"Group4 Items\" inline name=\"group4Items\" min=\"1\" max=\"50\" value=\"1\" step=\"1\" condition=\"groups >3\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup4\" value condition=\"groups>3\">\n\n        <input type=\"range\" title=\"Group5 Items\" inline name=\"group5Items\" min=\"1\" max=\"50\" value=\"1\" step=\"1\" condition=\"groups >4\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup5\" value condition=\"groups>4\">\n\n        <input type=\"range\" title=\"Group6 Items\" inline name=\"group6Items\" min=\"1\" max=\"50\" value=\"1\" step=\"1\" condition=\"groups >5\">\n        <input type=\"text\" title=\"Special Items In Group\" name=\"specialsInGroup6\" value condition=\"groups>5\">\n\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" value=\"../_images/background3.jpg\" title=\"Background Image\">\n            <input type=\"color\" value=\"#ffffff\" title=\"Background Color\" selected>\n            <input type=\"video\" value=\"https://www.youtube.com/watch?v=pjiVuLVqWQg\" title=\"Background Video\">\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay&&bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div class=\"py-3 ornament align-center\" mbr-if=\"showUnderline\">\n            <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 133 20\" height=\"20\" width=\"150\" fill=\"{{underlineColor}}\">\n                <g><path d=\"M127.18,6.19C121-.05,109.94-.24,101.84.1,90.84.56,80,3.3,69.32,5.77c-8,1.86-16,3.63-24,5.72-9.21,2.41-18.6,4.72-28.14,5.19-4.1.2-8.48,0-12.16-2a6.6,6.6,0,0,1-2.7-9A5.46,5.46,0,0,1,7.07,2.77c4-.22,6.73,8.39-.08,4.58a.49.49,0,0,0-.49.84c4.88,3,10.8,0,5.4-4.91C7.6-.63.4,2.06,0,8-1,24,28.25,17,36.29,15.24,53.74,11.37,70.9,6.44,88.53,3.33c11.1-2,24.75-3.54,35.06,2.19a16,16,0,0,1,6.86,8.85c1.29,4.31-4.35,4.2-7,3.52-4.84-1.26-2.42-8,1.22-3,.61.83,2,0,1.4-.82-2.15-2.93-6.71-4.9-7.12.35s8.36,7.24,11.72,4.47C135,15.32,129.91,9,127.18,6.19Z\"></path></g>\n            </svg>\n        </div>\n\n        <div class=\"row\" mbr-class=\"{'pt-5':showTitle||showUnderline}\">\n            <div class=\"col-md-6\">\n                <div class=\"menu-group\">\n                    <h3 class=\"mbr-fonts-style group-title mbr-black align-left\" mbr-theme-style=\"display-2\" data-app-selector=\".group-title\">\n                        General Services</h3>\n                    <div class=\"row menu-row\" mbr-list mbr-list-grow=\"group2Items\" data-specials=\"{{specialsInGroup2}}\">\n                        <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">General Service</h3>\n                            <div class=\"menu-box\">\n                                <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">Hardware diagnostic checks, OS updates, Deletion of unwanted programs, optimize start-up times.</p>\n                                <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-7\"><b>\n                                    $60</b></span>\n                            </div>\n                        </div>\n                        <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Windows upgrades</h3>\n                            <div class=\"menu-box\">\n                                <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">Upgrades from Windows 7 to Windows 10 or similar.</p>\n                                <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-7\"><b>\n                                    $70\n                                </b></span>\n                            </div>\n                        </div>\n                        <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Dedusting / Detailing of your machines</h3>\n                            <div class=\"menu-box\">\n                                <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">\n                                    Service includes dust removal, reapplying thermal paste to your CPU, cable management and general cleaning with Isopropyl alcohol.</p>\n                                <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-7\"><b>\n                                    $40</b></span>\n                            </div>\n                        </div>\n                        <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Outcall Service</h3>\n                            <div class=\"menu-box\">\n                                <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">When you have a general issue with your computer or network such as email issues, internet connectivity issues we charge a standard hourly rate and call out fee to come out and solve your issue.&nbsp;</p>\n                                <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-7\"><b>\n                                    $50 p/h +&nbsp;</b><br><b>$30 outcall fee</b></span>\n                            </div>\n                        </div>\n                        \n                    </div>\n                </div>\n                <div class=\"menu-group\" mbr-if=\"groups > 3\">\n                  <h3 class=\"mbr-fonts-style group-title mbr-black align-left\" mbr-theme-style=\"display-2\" data-app-selector=\".group-title\">\n                      Backpack\n                  </h3>\n                  <div class=\"row menu-row\" mbr-list mbr-list-grow=\"group4Items\" data-specials=\"{{specialsInGroup4}}\">\n                      <div class=\"col-md-12 menu-item\">\n                          <div class=\"special-label align-right\">\n                            <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                          </div>\n                          <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Blue Backpack</h3>\n                          <div class=\"menu-box\">\n                              <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">\n                                  Lorem ipsum dolor sit amet, consectetur\n                              </p>\n                              <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-2\">\n                                  $35\n                              </span>\n                          </div>\n                      </div>\n                  </div>\n                </div>\n                <div class=\"menu-group\" mbr-if=\"groups > 5\">\n                    <h3 class=\"mbr-fonts-style group-title mbr-black align-left\" mbr-theme-style=\"display-2\" data-app-selector=\".group-title\">\n                        Backpack\n                    </h3>\n                    <div class=\"row menu-row\" mbr-list mbr-list-grow=\"group6Items\" data-specials=\"{{specialsInGroup6}}\">\n                        <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Blue Backpack</h3>\n                            <div class=\"menu-box\">\n                                <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">\n                                    Lorem ipsum dolor sit amet, consectetur\n                                </p>\n                                <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-2\">\n                                    $35\n                                </span>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-md-6\">\n                <div class=\"menu-group\">\n                    <h3 class=\"mbr-fonts-style group-title mbr-black align-left\" mbr-theme-style=\"display-2\" data-app-selector=\".group-title\">\n                        Laptop Services</h3>\n                    <div class=\"row menu-row\" mbr-list mbr-list-grow=\"group1Items\" data-specials=\"{{specialsInGroup1}}\">\n                        <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Laptop screen replacement</h3>\n                            <div class=\"menu-box\">\n                                <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">\n                                    Depending on the size and connector type and what stock we have will depend on the price and turn around time. Call us for a rough price.</p>\n                                <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-7\"><b>\n                                    $80 to $140</b></span>\n                            </div>\n                        </div>\n                        <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Laptop RAM upgrade</h3>\n                            <div class=\"menu-box\">\n                                <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">\n                                    $10~ per gb of RAM fitted depending on the socket and quantity.</p>\n                                <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-7\"><b>\n                                    $10~ per/gb\n                                </b></span>\n                            </div>\n                        </div>\n                    <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Laptop keyboard replacements</h3>\n                            <div class=\"menu-box\">\n                                <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">Depending on our stock we may have to order it in. Fitting is $20 plus cost of keyboard. Call us to see if we have a keyboard in stock and rough cost.</p>\n                                <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-7\"><b>\n                                    $20 +</b><br><b>cost of keyboard</b>\n                                </span>\n                            </div>\n                        </div><div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Laptop SSD upgrade</h3>\n                            <div class=\"menu-box\">\n                                <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">Service includes data migration and we'll install your old hard drive in an enclosure (caddy) which connects via USB so you have a backup.</p>\n                                <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-7\"><b>\n                                    $30 + </b><br><b>cost of SSD</b>\n                                </span>\n                            </div>\n                        </div></div>\n                </div>\n                <div class=\"menu-group\" mbr-if=\"groups > 2\">\n                    <h3 class=\"mbr-fonts-style group-title mbr-black align-left\" mbr-theme-style=\"display-2\" data-app-selector=\".group-title\">\n                        Glasses\n                    </h3>\n                    <div class=\"row menu-row\" mbr-list mbr-list-grow=\"group3Items\" data-specials=\"{{specialsInGroup3}}\">\n                        <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Brown Glasses</h3>\n                            <div class=\"menu-box\">\n                                <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">\n                                    Lorem ipsum dolor sit amet, consectetur\n                                </p>\n                                <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-2\">\n                                    $5</span>\n                            </div>\n                        </div>\n                        <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Black Glasses</h3>\n                            <div class=\"menu-box\">\n                                <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">\n                                    Lorem ipsum dolor sit amet, consectetur\n                                </p>\n                                <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-2\">\n                                    $10</span>\n                            </div>\n                        </div>\n                        <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Red Glasses</h3>\n                            <div class=\"menu-box\">\n                                <p class=\"box-text mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".box-text\">\n                                    Lorem ipsum dolor sit amet, consectetur\n                                </p>\n                                <span mbr-text class=\"mbr-fonts-style box-price mbr-black\" mbr-theme-style=\"display-2\">\n                                    $8</span>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"menu-group\" mbr-if=\"groups > 4\">\n                    <h3 class=\"mbr-fonts-style group-title mbr-black align-left\" mbr-theme-style=\"display-2\" data-app-selector=\".group-title\">\n                        Backpack\n                    </h3>\n                    <div class=\"row menu-row\" mbr-list mbr-list-grow=\"group5Items\" data-specials=\"{{specialsInGroup5}}\">\n                        <div class=\"col-md-12 menu-item\">\n                            <div class=\"special-label align-right\">\n                              <div mbr-text class=\"label-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".special-label\">New</div>\n                            </div>\n                            <h3 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\">Blue Backpack</h3>\n